#!/bin/bash

company=$1
id=$2

curl --request DELETE http://127.0.0.1:7000/api/1/companies/$company/canned_answer_folders/$id  \
  -H 'Connection: keep-alive' \
  -H 'Accept: application/json, text/plain, */*' \
  -H 'x-front-xsrf: nltD4VXE-JiInrdhm6M11Th8dfTrPGa_1Oy0' \
  -H 'Content-Type: application/json;charset=UTF-8' \
  -H 'Accept-Language: en-US,en;q=0.9' \
  -H 'Cookie: front.csrf=nltD4VXE-JiInrdhm6M11Th8dfTrPGa_1Oy0; io=_e6hiphtKgkkLBPoAAAC; front.; front.id.sig=K0nTaCyUxp3H78KolGW_TPDw5Rw'
